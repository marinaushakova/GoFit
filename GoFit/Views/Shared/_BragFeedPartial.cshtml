@model IList<GoFit.Models.user_workout>

<h5>Brag Feed</h5>
<ul id="scroller">
    @foreach (var item in Model)
    {
        <li>
            <div style="width:290px;height:100px;border:1px solid red;">
                <p style="text-align:center;">
                    @Html.Encode(item.user.username) completed
                    @Html.Encode(item.workout.name) at
                    @Html.Encode(item.date_finished)
                </p>
            </div>
        </li>
    }
</ul>

<script type="text/javascript">
    (function ($) {
        $(function () {
            $("#scroller").simplyScroll({ frameRate: 50 });
        });
    })(jQuery);
</script>